<%= form_with url: url, model: model do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>

  <%= f.label :street_address %>
  <%= f.text_field :street_address %><br>

  <%= f.label :city %>
  <%= f.text_field :city %><br>

  <%= f.label :state %>
  <%= f.text_field :state %><br>

  <%= f.label :zip_code %>
  <%= f.number_field :zip_code %><br>
  
  <%= f.hidden_field :description, value: "N/A" %><br>
  <%= f.hidden_field :status, value: "In Progress" %><br>

  <%= f.submit "Submit" %>
<% end %>
</div>